{
  "name": "drun-server",
  "version": "1.0.0",
  "description": "Backend server for the DRUN autonomous drone delivery system.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/braind3d/DRUN"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node ./service/src/src/index.ts",
    "start:dev": "ts-node-dev --respawn ./service/src/index.ts",
    "start:order-service": "ts-node ./services/order-service/src/index.ts",
    "start:order-service:dev": "ts-node-dev --respawn ./services/order-service/src/index.ts",
    "start:payment-service": "ts-node ./services/payment-service/src/index.ts",
    "start:payment-service:dev": "ts-node-dev --respawn ./services/payment-service/src/index.ts",
    "start:delivery-service": "ts-node ./services/delivery-service/src/index.ts",
    "start:delivery-service:dev": "ts-node-dev --respawn ./services/delivery-service/src/index.ts",
    "start:user-service": "ts-node ./services/user-service/src/index.ts",
    "start:user-service:dev": "ts-node-dev --respawn ./services/user-service/src/index.ts",
    "start:discord-service": "ts-node ./services/discord-service/src/index.ts",
    "start:discord-service:dev": "ts-node-dev --respawn ./services/discord-service/src/index.ts",
    "test": "jest",
    "lint": "eslint ./",
    "lint:fix": "eslint \"**/*.{js,ts}\" --fix ./"
  },
  "author": "FAnton",
  "license": "CC-BY-4.0",
  "dependencies": {
    "@babel/preset-typescript": "^7.12.13",
    "amqplib": "^0.6.0",
    "axios": "^0.21.1",
    "babel-runtime": "^6.26.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^5.3.1",
    "moment": "^2.29.1",
    "pg": "^8.5.1",
    "pgmock2": "^2.0.6",
    "stripe": "^8.134.0",
    "ts-node": "^9.1.1",
    "twilio": "^3.55.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@types/amqplib": "^0.5.17",
    "@types/bluebird-global": "^3.5.12",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/pg": "^7.14.9",
    "@types/stripe": "^8.0.417",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.6.3",
    "mock-amqplib": "^1.3.0",
    "regenerator-runtime": "^0.13.7",
    "sinon": "^9.2.4",
    "slonik": "^23.5.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.1"
  }
}
